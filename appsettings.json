{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:5005",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://+:5005"
      }
    }
  },
  "Downstream": {
    "Enabled": false,
    "URL": "http://localhost:5005/hello?sleep=5"
  },
  "Cache": {
    "RedisConfigured": false,
    "MemCache": false
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Otel": {
    "Enabled": "true",
    "Endpoint": "http://localhost:4317"
  }
}